---
import Cards from '../components/ProyectoCards.astro'
import SectionTitle from '../components/SectionTitle.astro'
import { proyectos } from '../consts/proyectos'

---
<section id="proyectos" class=`max-w-5xl mx-auto p-3 text-white`>
  <article class=" ">
    <SectionTitle > &lt Proyectos /&gt </SectionTitle>
  
    <div class="flex flex-col gap-y-12 ">
      {proyectos.map( proyecto => <Cards {...proyecto}/>)}
    </div>
  
  </article>
</section>
---
import Layout from "../../layouts/Layout.astro"
import Footer from "../../components/Footer.astro"
---

<Layout>
  <main class="max-w-7xl mx-auto p-4">
    <img src="/public/c.g.p.webp" class="mt-20" alt="">
    <div class="mt-20  text-white">
        <ul class="flex gap-4 ">
            <li class="rounded px-6 py-2 bg-gradient-to-b from-rose-300/10 to-rose-600/20">Proyecto 1</li>
            <li class="rounded px-6 py-2 bg-gradient-to-b from-rose-300/10 to-rose-600/20">Proyecto 2</li>
            <li class="rounded px-6 py-2 bg-gradient-to-b from-rose-300/10 to-rose-600/20">Proyecto 3</li>
            <li class="rounded px-6 py-2 bg-gradient-to-b from-rose-300/10 to-rose-600/20">Proyecto 4</li>
            <li class="rounded px-6 py-2 bg-gradient-to-b from-rose-300/10 to-rose-600/20">Proyecto 5</li>
        </ul>
    </div>
    <Footer/>

  </main>
</Layout>
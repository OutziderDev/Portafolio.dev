---
import Proyectos from "../sections/Proyectos.astro"
import Hero from "../sections/Hero.astro"
import Layout from "../layouts/Layout.astro"
import SectionContainer from '../components/SectionContainer.astro';
import Formacion from '../sections/Formacion.astro';
import AboutSection from '../sections/AboutSection.astro'
import Contacto from "../sections/Contacto.astro";
import Footer from "../sections/Footer.astro";
---
<Layout>
  <main>
    <Hero/>
    <Proyectos/>
    <Formacion/>
    <AboutSection/>

    <SectionContainer id="contacto">
      <Contacto/>        
    </SectionContainer>
    
    <SectionContainer id="footer">
      <Footer/>
    </SectionContainer> 
    
  </main>    
</Layout>